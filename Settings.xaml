<Window x:Class="Prakt13.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prakt13"
        mc:Ignorable="d"
        Title="Settings" Height="183" Width="347" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="Row" HorizontalAlignment="Left" Margin="25,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Column" HorizontalAlignment="Left" Margin="25,79,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button HorizontalAlignment="Left" Margin="38,123,0,0" VerticalAlignment="Top" Content="Отмена" Height="25" Width="68" Click="Отмена"/>
        <Button HorizontalAlignment="Left" Margin="215,123,0,0" VerticalAlignment="Top" Content="Сохранить" Height="25" Width="68" Click="Сохранить"/>
        <Label Content="- количество строк" HorizontalAlignment="Left" Margin="150,24,0,0" VerticalAlignment="Top"/>
        <Label Content="- количество столбцов" HorizontalAlignment="Left" Margin="150,75,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
